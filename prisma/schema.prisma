
generator client {
  provider = "prisma-client-js"
  //output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(uuid())
  username String 
  email String @unique
  password String
  products Product []

}

model Product {
  id String @id @default(uuid())
  name String
  price Float
  decription String
  userId String
  user User @relation(fields: [userId],references: [id])
}
